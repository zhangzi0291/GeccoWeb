<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.demo.dao.RentDao" >
  <resultMap id="BaseResultMap" type="com.demo.gecco.htmlBean.ChuZhuBean" >
    <result column="rent_title" property="title" jdbcType="VARCHAR" />
    <result column="rent_href" property="href" jdbcType="VARCHAR" />
    <result column="rent_price" property="price" jdbcType="VARCHAR" />
    <result column="rent_type" property="type" jdbcType="VARCHAR" />
    <result column="rent_image" property="image" jdbcType="VARCHAR" />
  </resultMap>
 <insert id="insert" parameterType="com.demo.gecco.htmlBean.ChuZhuBean">
 	insert into rent_house (rent_title,rent_href,rent_price,rent_type,rent_image)
 	values(  #{title,jdbcType=VARCHAR},#{href,jdbcType=VARCHAR},#{price,jdbcType=VARCHAR},#{type,jdbcType=VARCHAR},#{image,jdbcType=VARCHAR}  )
 </insert>
 <select id="findAll" resultMap="BaseResultMap">
 	select * from rent_house
 </select>
 <select id="findByExample" resultMap="BaseResultMap" parameterType="com.demo.gecco.htmlBean.ChuZhuBean">
 	select * from rent_house
 	where 1=1
      <if test="title != null" >
        and rent_title = #{title,jdbcType=VARCHAR},
      </if>
      <if test="href != null" >
        and rent_href = #{href,jdbcType=VARCHAR},
      </if>
      <if test="price != null" >
        and rent_price = #{price,jdbcType=VARCHAR},
      </if>
      <if test="type != null" >
        and rent_type = #{type,jdbcType=VARCHAR},
      </if>
      <if test="image != null" >
        and rent_image = #{image,jdbcType=VARCHAR},
      </if>
 </select>
 
</mapper>